{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev"
    },
  
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Jest Tests",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": ["--runInBand"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
  ]
}